    <org.ihtsdo.wb-toolkit.version>2.6.0-rb-2</org.ihtsdo.wb-toolkit.version>
  
    <!--
    <org.ihtsdo.wb-toolkit.version>2.6.1-trek-termmed-101</org.ihtsdo.wb-toolkit.version>
    <org.ihtsdo.wb-toolkit.version>2.6.0-trek-114</org.ihtsdo.wb-toolkit.version>
